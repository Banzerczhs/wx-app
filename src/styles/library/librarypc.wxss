.head{
    width:100%;
    height:30%;
    position: relative;
}
.head image{
    border-radius:50%;
    width:200rpx;
    height:200rpx;
    position: absolute;
    top:50%;
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
    -webkit-transform: translateX(-50%) translateY(-50%);
}
.bottom-box{
    width:90%;
    margin:0 auto;
    height:60%;
    background:rgba(250,250,250,.5);
    position: relative;
    padding-top:30rpx;
}
.bottom-box view{
    width:300rpx;
    height:100rpx;
    line-height: 100rpx;
    background: rgba(250,250,250,.8);
    margin:50rpx auto;
    text-align: center;
    border-radius:10%;
    color:#134675;
}
