/*遮罩*/
.mask{
  position: absolute;
  z-index:-1;
  left:0;top:0;
  opacity: 0;
  transition: 1.5s;
}
.maskin{
  position: absolute;
  left:0;top:0;
  opacity:1;
  z-index:10; 
  transition: 1.5s;
}
.mask-box{
  position: absolute;
  top:50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  width: 200rpx;
  min-height: 200rpx;
}
.mask-element{
  text-align: center;
  box-shadow: 2px 2px 5px #333;
  margin: 15px 0;
  width:200rpx;
  height: 200rpx;
  line-height:200rpx;
  border-radius:50%;
  background:rgba(250,250,250,0.5);
  transform: scale(0);
  transform-origin: center center;
  transition: .5s cubic-bezier(0.5,0.4,0.5,1.5);
}
.mask-element-show{
  transform: scale(1);
  background:rgba(250,250,250,1);
}
/*背景动画*/
@keyframes animationleft{
  0%{
   transform: translateX(0px) skewX(-10deg);
 
  }
  100%{
    transform: translateX(15px)  skewX(-20deg) ;
  }
}
@keyframes animationright{
  0%{
   transform: translateX(0px) skewX(10deg);
 
  }
  100%{
    transform: translateX(15px)  skewX(0deg) ;
  }
}
@keyframes animationSea{
  0%{
   transform: skewX(0deg) ;
 
  }
  50%{
    transform: skewX(4deg);
  }
  100%{
    transform: skewX(-4deg) ;
  }
}
