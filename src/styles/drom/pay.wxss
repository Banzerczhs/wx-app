page{
    width:100%;
    height:100%;
    overflow: hidden;
}
.box-content{
    width:70%;
    margin:0 auto;
    height:100%;
}
.content-room{
    background:#66ccff;
    position: relative;
    width:100%;
    height:7vh;
    margin-top:50rpx;
}
.content-pay{
    width:100%;
    height:200rpx;
    text-align: center;
    background:rgba(250, 248, 248, 0.712);
    margin-top:100rpx;
}
.content-pay .TextShow{
    position: relative;
    top:10px;
}
.content-select{
    width:100%;
    height:8vh;
    margin-top:30rpx;
    background:100rpx;
    text-align: center;
}
.content-select button{
     background:#66ccff;
     border-radius:3px;
     width:120rpx;
     height:50rpx;
     line-height: 50rpx;
     display: inline-block;
     color:rgb(139, 27, 27);
     font-size:18/1.5px;
     cursor:pointer;
}
.content-select button:nth-child(1){
    float:left;
    margin-left:30px;
}
.content-select button:nth-child(2){
    float:right;
    margin-right:30px;
}
.show-electricity{
    width:100%;
    height:30vh;
    line-height: 30vh;
    color:#fff;
    text-align: center; 
}
.nav{
    width:100%;
    height:8vh;
    overflow: hidden;    
    text-align: center;
    background: rgb(58, 88, 153);
}
.nav-ul{
    height:100%;
    position: relative;
    display: flex;
   justify-content:space-around; 
}
.navbox{
    width:230rpx;
    height:95%;
    display: flex;
    align-items: center;
    justify-content:space-around; 
    float:left;
    color:#fff;
}
.navbox  .active{
    color:lightcoral;
    font-size:16px;
    transition: all 0.5s;
}
.overflow-box{
    position: relative;
    height:100%;
    width:100%;
    overflow: hidden;
}
.contentpage{
    width:300%;
    height:100%;
    position: relative;
    transform: translateX(0%);
}
.contentbox{
    width:750rpx;
    height:100%;
    float: left;
}
.model-width{
    width:750rpx;
}
.model-position{
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}
.inputUser{
    width:220rpx;
    height:80rpx;
    margin-top:10rpx;
    margin-left:10rpx;
    line-height: 80rpx;
}
.inputUser::before{
    content:'å…ƒ';
    position: absolute;
    right:40rpx;
}
.xuehao{
    height:7vh;
    line-height: 7vh;
    padding-left:5px;
}
.xuehao input {
    width:80%;
    padding-left:10px;
}